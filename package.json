{
  "name": "MERNolithic_sql",
  "version": "1.0.0",
  "description": "",
  "main": "backend/server.js",
  "scripts": {
    "start": "NODE_ENV=test node backend/server.js",
    "start-dev": "NODE_ENV=development node backend/server.js",
    "start-test": "NODE_ENV=test node backend/server.js",
    "heroku-postbuild": "cd frontend && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "clean-reports": "rm -rf cypress/reports",
    "test": "cypress run --reporter mochawesome",
    "merge-report": "npx mochawesome-merge cypress/reports/* > mochawesome.json",
    "generate-report": "npx marge mochawesome.json",
    "after:tests": "yarn run merge-report; yarn run generate-report",
    "cypress-test": "yarn run clean-reports; yarn run test; yarn run after:tests",
    "cleanup:all": "run-p cleanup:reports cleanup:evidence",
    "cleanup:reports": "rm -fr cypress/reports/* && rm -fr cypress/reports/*",
    "cleanup:evidence": "rm -fr cypress/videos* && rm -fr cypressscreenshots*",
    "merge_reports": "mochawesome-merge 'cypress/reports/*.json' > mochawesome.json",
    "generate_html_report": "marge mochawesome.json",
    "test:cy": "run-p --race --silent start test-no-exit",
    "cy:run": "cypress run",
    "test-no-exit": "npm run cy:run --force",
    "test:create-reports": "run-s cleanup:all test:cy merge_reports generate_html_report create-tree",
    "create-tree": "node cypress/cy-report-tree-generator.js --path=cypress/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charlesmbrady/MERNolithic_sql.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/charlesmbrady/MERNolithic_sql/issues"
  },
  "homepage": "https://github.com/charlesmbrady/MERNolithic_sql#readme",
  "devDependencies": {
    "cy-report-setup-helper": "^1.3.4",
    "cypress": "^4.2.0",
    "directory-tree": "^2.2.4",
    "json-beautify": "^1.1.1",
    "mocha": "^7.1.1",
    "mochawesome": "^5.0.0",
    "mochawesome-merge": "^4.0.3",
    "mochawesome-report-generator": "^4.1.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "yargs": "^15.1.0"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "sequelize": "^5.21.5"
  }
}
